<thead class="text-xs text-gray-700 uppercase bg-gray-400 text-left">

<tr>
  <th scope="col" class="px-6 py-3">
    Biomarker
  </th>
  <th scope="col" class="px-6 py-3">
    Reference range
  </th>
  <% recordables.each do |recordable| %>
    <th scope="col" class="px-6 py-3">
      <%= format_date recordable.created_at %>
    </th>
  <% end %>
</tr>

</thead>
